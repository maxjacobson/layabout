<% if current_user.present? %>
  var bookmarkHTML = '<%= j render(partial: 'modal', locals: {html: @html}) %>';
  var bookmarkModal = $(bookmarkHTML);
  bookmarkModal.modal('show');
  bookmarkModal.on('hidden.bs.modal', function(){
    $('#myModal, .modal-backdrop').remove();
  });
<% else %>
  {
    no: 'no'
  }
<% end %>


